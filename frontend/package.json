    "axios": "^1.0.0",
    "cdbreact": "latest",
    "chart.js": "latest",
    "flowbite-react": "latest",
    "react": "latest",
    "react-chartjs-2": "latest",
    "react-dom": "latest",
    "react-router-dom": "latest",
    "react-scripts": "^5.0.1",
    "web-vitals": "latest"
  },
  "devDependencies": {
    "cross-env": "latest",
    "eslint": "latest",
    "prettier": "latest"
  },

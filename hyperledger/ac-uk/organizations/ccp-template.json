{
    "name": "${ORG}-ac-uk",
    "version": "1.0.0",
    "client": {
      "organization": "${ORG}",
      "connection": {
        "timeout": {
          "peer": {
            "endorser": "300"
          }
        }
      }
    },
    "organizations": {
      "Org${ORG}": {
        "mspid": "${ORGMSP}",
        "peers": [
          "peer0.${ORG}.ac.uk"
        ],
        "certificateAuthorities": [
          "ca.${ORG}.ac.uk"
        ]
      }
    },
    "peers": {
      "peer0.${ORG}.ac.uk": {
        "url": "grpcs://localhost:${P0PORT}",
        "tlsCACerts": {
          "pem": "${PEERPEM}"
        },
        "grpcOptions": {
          "ssl-target-name-override": "peer0.${ORG}.ac.uk",
          "hostnameOverride": "peer0.${ORG}.ac.uk"
        }
      }
    },
    "certificateAuthorities": {
      "ca.${ORG}.ac.uk": {
        "url": "https://localhost:${CAPORT}",
        "caName": "ca-${ORG}",
        "tlsCACerts": {
          "pem": ["${CAPEM}"]
        },
        "httpOptions": {
          "verify": false
        }
      }
    }
  }